{
   "kind":"List",
   "apiVersion":"v1",
   "items":[
      {
         "kind":"ReplicationController",
         "apiVersion":"v1",
         "metadata":{
            "name":"kubernetes-cockpit",
            "labels":{
               "name":"kubernetes-cockpit"
            }
         },
         "spec":{
            "replicas":1,
            "selector":{
               "name":"kubernetes-cockpit"
            },
            "template":{
               "metadata":{
                  "labels":{
                     "name":"kubernetes-cockpit"
                  }
               },
               "spec":{
                  "containers":[{
                    "name": "kubernetes-cockpit",
                    "image": "kubevirt/cockpit:devel",
                    "ports":[{
                        "containerPort":9090,
                        "protocol":"TCP"
                     }],
                    "env":[{
                        "name": "COCKPIT_KUBE_SERVER",
                        "value": "https://192.168.200.2:8184"
		    }]
                  }]
               }
            }
         }
      },
      {
         "kind":"Service",
         "apiVersion":"v1",
         "metadata":{
            "name":"kubernetes-cockpit",
            "labels":{
               "name":"kubernetes-cockpit"
            }
         },
         "spec":{
	     "externalIPs":[
		     "192.168.200.2"
	     ],
             "ports": [{
                 "protocol": "TCP",
                 "port": 9091,
                 "targetPort": 9090
             }],
            "selector":{
               "name":"kubernetes-cockpit"
            }
         }
      }
   ]
}
